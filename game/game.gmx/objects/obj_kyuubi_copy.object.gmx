if(ds_queue_empty(kyuubi_word) &amp;&amp; ds_queue_empty(siro_word) &amp;&amp; ds_queue_empty(speed_queue) &amp;&amp; alarm[0] == -1){
    if(ins_voice.visible == false &amp;&amp; obj_chara.ins_voice.visible == false){
        phase += 1;
        //台詞読み込み
        if(phase != pre_phase){
            if(phase &gt;= 4){
                ds_queue_copy(kyuubi_word,kyuubi_words[? "s"+string(s_no)+"_p"+string(phase)+"_"+string(select)]);
                ds_queue_copy(siro_word,siro_words[? "s"+string(s_no)+"_p"+string(phase)+"_"+string(select)]);
            } else{
                ds_queue_copy(kyuubi_word,kyuubi_words[? "s"+string(s_no)+"_p"+string(phase)]);
                if(phase &gt;= 2){
                    ds_queue_copy(siro_word,siro_words[? "s"+string(s_no)+"_p"+string(phase)]);
                }
            }
        }
        pre_phase = phase;

        //After finish
        if(phase == 6){
            with(obj_chara){
                visible = true;
                ins_name.visible = true;
                x = obj_ren.x;
                chara_data[? "hp"] = 0;
                image_speed = 0.02;
            }            
        }
        //Reset
        if(phase == 7){
            //initilise variables
            alarm[0] = 210;
            phase = 0;
            select = 0;
            with(obj_chara){
                scr_chara_reset();
            }
            if(s_no &gt;= 2){
                with(obj_chara){
                    chara_data[? "condition"] = "bote";
                    image_speed = 0.1;
                    move_x = 1;     
                }       
            }
            ds_map_clear(siro_word);
            if( s_total &gt; 1 ){
                if(s_no &lt; s_total){
                    s_no += 1;
                } else if(s_no &gt;= s_total){
                    s_no -= 1;                    
                }
            }
            
        }
    }
}

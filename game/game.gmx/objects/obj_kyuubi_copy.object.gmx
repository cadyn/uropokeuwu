if(ds_queue_size(kyuubi_word) &gt; 0){
    if( is_string(ds_queue_head(kyuubi_word))){
        //可変イメージ設定
        if(asset_get_index(ds_queue_head(kyuubi_word)) != -1){
            sprite_index = asset_get_index(ds_queue_dequeue(kyuubi_word));        
        }
    }
    if( is_real(ds_queue_head(kyuubi_word))){
        if(ds_queue_head(kyuubi_word) &gt;= 10){
            //可変タイマー設定
            alarm[0] = ds_queue_dequeue(kyuubi_word);
        } else if(ds_queue_head(kyuubi_word) &lt; 10){
            //可変速度設定
            ds_queue_enqueue(speed_queue,ds_queue_dequeue(kyuubi_word));
        } else{
            var dump = ds_queue_dequeue(kyuubi_word);
        }
    }
    if( is_string(ds_queue_head(kyuubi_word)) &amp;&amp; alarm[0] == -1){
        //台詞出力
        ins_voice.alarm[0] = -1;
        ins_voice.visible = false;
        var mes = ds_queue_dequeue(kyuubi_word);
        scr_voice(mes);
        alarm[0] = 200;
    }

}
if(ds_queue_size(siro_word) &gt; 0){
    if( is_string(ds_queue_head(siro_word))){
        //可変イメージ設定

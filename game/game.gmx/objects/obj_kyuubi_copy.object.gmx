            obj_chara.opp_img = ds_list_find_value(global.chara_img[? "siro_"+ds_queue_dequeue(siro_word)],0);           
        }
    }
    if( is_real(ds_queue_head(siro_word))){
        if(ds_queue_head(siro_word) &gt;= 10){
            //可変タイマー設定
            obj_chara.alarm[0] = ds_queue_dequeue(siro_word);
        } else if(ds_queue_head(kyuubi_word) &lt; 10){
            //可変速度設定（相手側）
            ds_queue_enqueue(speed_queue,ds_queue_dequeue(siro_word));
        } else{
            var dump = ds_queue_dequeue(siro_word);
        }
    }
    
//台詞位置調整
    if(is_string(ds_queue_head(siro_word))){
